<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:SkinBox.Controls;assembly=SkinBox.Controls">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/SkinBox.Controls;component/Themes/Brushes.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="{x:Static controls:Colors.BackgroundColorKey}">Red</Color>
    <Color x:Key="{x:Static controls:Colors.AccentColorKey}">SaddleBrown</Color>
</ResourceDictionary>