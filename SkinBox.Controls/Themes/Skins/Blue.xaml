<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SkinBox.Controls">
    <SolidColorBrush x:Key="{x:Static local:Keys.BackgroundBrushKey}" Color="Blue" />

    <ControlTemplate x:Key="{x:Static local:Keys.CustomControlTemplateKey}" TargetType="{x:Type local:CustomControl}">
        <Rectangle Fill="{TemplateBinding Background}" Stretch="Fill" />
    </ControlTemplate>
</ResourceDictionary>